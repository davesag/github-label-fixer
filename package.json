{
  "name": "github-labeller",
  "version": "1.0.0",
  "description": "A command line utility to replace the default GitHub issue labels with ones Dave Sag finds more useful",
  "main": "bin/index.js",
  "engines": {
    "node": ">=6.9.4",
    "npm": "^3"
  },
  "scripts": {
    "clean": "rm -rf bin/",
    "start": "node bin/index.js",
    "babel-v": "babel --version",
    "babel": "babel src --out-dir bin",
    "build": "npm run clean && npm run babel",
    "dev": "babel-node src/index.js",
    "test": "find ./test -name '*_spec.js' | NODE_ENV=test xargs mocha --compilers js:babel-core/register --require ./test/test_helper.js"
  },
  "keywords": [],
  "author": "Dave Sag <davesag@gmail.com>",
  "license": "MIT",
  "babel": {
    "presets": [
      "es2015-node",
      "stage-0"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davesag/github-labeller.git"
  },
  "bugs": {
    "url": "https://github.com/davesag/github-labeller/issues"
  },
  "homepage": "https://github.com/davesag/github-labeller#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-preset-es2015-node": "^6.1.1",
    "babel-preset-stage-0": "^6.16.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-string": "^1.3.0",
    "mocha": "^3.1.2",
    "proxyquire": "^1.7.10",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "sinon-stub-promise": "^4.0"
  },
  "dependencies": {
    "dotenv": "^4.0.0",
    "prompt": "^1.0.0",
    "request": "^2.79.0",
    "request-promise-native": "^1.0.3",
    "set-manipulator": "^0.3.1",
    "username": "^2.3.0"
  }
}
